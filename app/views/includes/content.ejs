<!-- Right side column. Contains the navbar and content of the page -->
<div class="content-wrapper">

  <!-- Modal -->
  <div class="modal fade" id="myModal" role="dialog">
    <div class="modal-dialog">

      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 id="notificacaoMensagemTitle" class="modal-title"></h4>
        </div>
        <div class="modal-body">
          <p id="notificacaoMensagemBody"></p>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
          <button type="button" class="btn btn-primary">Modelar Projeto</button>
        </div>
      </div>

    </div>
  </div>

  <!-- Content Header (Page header) -->
  <section class="content-header">
    <h1>
      Apresentação dos Protótipos
      <small>Tipo Usuário
        <% if (sessionNomeTipoUsuario == 1){ %>
          Cliente
          <% } else if(sessionNomeTipoUsuario == 2) { %>
            Desenvolvedor
            <% } else if(sessionNomeTipoUsuario == 3) { %>
              Tutor
              <% } else { %>
                Error
                <% } %></small>
              </h1>
              <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                <li class="active">Protótipos</li>
              </ol>
            </section>

            <!-- Main content -->
            <section class="content">
              <!-- Info boxes -->


              <div class="row">
                <div class="col-md-12">
                  <div class="box">
                    <div class="box-header with-border">
                      <h3 class="box-title">Matheus o palestrante do dia</h3>

                    </div><!-- /.box-header -->
                    <div class="box-body">
                      <div class="row">
                        <div class="col-md-12">
                          <p class="text-center">
                            <strong>Sales: 1 Jan, 2014 - 30 Jul, 2014</strong>
                          </p>

                        </div><!-- /.col -->

                      </div><!-- /.row -->
                    </div><!-- ./box-body -->

                  </div><!-- /.box -->
                </div><!-- /.col -->
              </div><!-- /.row -->

            </section><!-- /.content -->
          </div><!-- /.content-wrapper -->

          <script>
          if(<%= notificacao[0].mensagem == 'Você precisa modelar um projeto' %>){
            $(window).on('load',function(){
              $('#myModal').modal('show');
              document.getElementById("notificacaoMensagemTitle").innerHTML = "Alerta!";
              document.getElementById("notificacaoMensagemBody").innerHTML = "Você precisa modelar um projeto.";
            });
          }else{
            console.log("ñ tem notificação");
          }
          </script>
