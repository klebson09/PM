<body class="register-page">
<div class="row">
  <form id="msform" action="/cadastrar_tutor" method="post" name="f1">
    <!-- progressbar -->
    <div class="col-md-offset-3 col-md-8 col-sm-offset-3">
      <ul id="progressbar" class="progressbarTutor">
