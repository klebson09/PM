<!-- Right side column. Contains the navbar and content of the page -->
<div class="content-wrapper">

</section>
<!-- Main content -->
<section class="content">
  <!-- Info boxes -->
  <!-- START ACCORDION -->
  <h2 class="page-header">Projetos</h2>
  <%= JSON.stringify(data); %>
  <div class="row">
<%
for(var i=0; i < data.length; i++){
%>

    <div class="col-md-6">
      <div class="box box-solid">
        <div class="box-header with-border">
          <h3 class="box-title"><%=data[i].nomeProjeto; %></h3>
          <h1>
          </h1>
        </div><!-- /.box-header -->
        <div class="box-body">
          <div class="box-group" id="accordion">
            <!-- we are adding the .panel class so bootstrap.js collapse plugin detects it -->
            <div class="panel box box-primary">
              <div class="box-header with-border">
                <h4 class="box-title">
                  <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
                    <% if(data[i].idPlataforma == 1) { %>
                      Sistema Android
                    <% } else if(data[i].idPlataforma == 2) { %>
                      Sistema Web
                    <% } else{ %>
                      Sistema NÃ£o definido
                    <% } %>
                  </a>
                </h4>
              </div>
              <div id="collapseOne" class="panel-collapse collapse in">
                <div class="box-body">
                   <%=data[i].descricao; %>
                   idProjeto <%=data[i].idProjeto; %>
                 </div>
                 <div class="box-footer ">
                   <form action="/candidatarse_projeto" method="post">
                     <input type="hidden" id="id_projeto" name="idProjeto" value="yy">
                     <input type="hidden" id="id_conta_us" name="idContaUsuario" value="xxx">
                     < !-- <a href="/criar_equipe" id="btnCandidatarSe" class="submit action-button btn  btn-success col-md-offset-5">Candidatar-se</a> -->
                     <input type="submit" id="btnCandidatarSe"  class="submit action-button btn  btn-success col-md-offset-5" value="Candidatar-se"  />

                   </form>
                   <script>
                   const novoValor = <%= data[i].idProjeto; %>;
                   const campo = document.getElementById("id_projeto");
                   campo.value = novoValor;

                   const novoValor2 = <%= data[i].idContaUsuario; %>;
                   const campo2 = document.getElementById("id_conta_us");
                   campo2.value = novoValor2;
                   </script>

                 </div>
               </div>

            </div>
          </div>
        </div><!-- /.box-body -->
      </div><!-- /.box -->
    </div><!-- /.col -->
<% } %>
  </div><!-- /.row -->
</section><!-- /.content -->
</div><!-- /.content-wrapper -->
