<% var dev = [{"icon":"fa fa-code","texto":"Novos Projetos","link":"/projeto_disp"},{"icon":"fa fa-search","texto":"Buscar Equipes","link":"/buscar_eqp"},{"icon":"fa fa-group","texto":"Criar Equipe","link":"/criar_equipe"},{"icon":"fa fa-check","texto":"Checkpoints","link":"/consultar_checkpoints"}];

var cli = [{"icon":"fa fa-edit","texto":"Modelar Projeto","link":"/modelar_projeto"}, {"icon":"fa fa-envelope","texto":"Propostas das Equipes","link":"/propostas_projeto"},{"icon":"fa  fa-file-text-o","texto":"Termos de Abertura","link":"/termo_abertura"},{"icon":"fa fa-check","texto":"Checkpoints","link":"/checkpoint"}];

var tut = [{"icon":"fa fa-group","texto":"Visualizar Equipe","link":"/visualizar_equipe"},{"icon":"fa fa-files-o","texto":"Visualizar Projetos","link":"/projeto_disp"},{"icon":"fa fa-file-text-o","texto":"Termos de Abertura","link":"/termo_abertura"},{"icon":"fa fa-check","texto":"Checkpoints","link":"/consultar_checkpoints"}]; 

var menuUsr = []; %>

<!-- Left side column. contains the logo and sidebar -->
<aside class="main-sidebar">
  <!-- sidebar: style can be found in sidebar.less -->
  <section class="sidebar">
    <!-- Sidebar user panel -->
    <div class="user-panel">
      <div class="pull-left image">
        <img src="dist/img/user2-160x160.jpg" class="img-circle" alt="User Image" />
      </div>
      <div class="pull-left info">
        <p><%= sessionNomeUsuario %></p>

        <a href="#"><i class="fa fa-circle text-success"></i>
          <% if (sessionNomeTipoUsuario == 'C'){ %>
            Cliente
            <% menuUsr = cli; %>
          <% } else if(sessionNomeTipoUsuario == 'D') { %>
            Desenvolvedor
            <% menuUsr = dev; %>
          <% } else if(sessionNomeTipoUsuario == 'T') { %>
            Tutor
             <% menuUsr = tut; %>
          <% } else { %>
            Error
          <% } %>
          </a>
      </div>
    </div>
    <!-- search form -->
    
    <!-- /.search form -->
    <!-- sidebar menu: : style can be found in sidebar.less -->
    <ul class="sidebar-menu">
      <li class="header">MENU PRINCIPAL</li>
        <li><a href="/homeTimeLine"><i class="fa fa-clock-o"></i>Linha do Tempo</a></li>
        <% for(var i=0; i< menuUsr.length; i++){ %>
        <li><a href="<%- menuUsr[i].link  %>"><i class="<%- menuUsr[i].icon  %>"></i><%- menuUsr[i].texto %></a></li>   
        <% }%>
    </ul>
  </section>
  <!-- /.sidebar -->
</aside>
