<!-- Right side column. Contains the navbar and content of the page -->
<div class="content-wrapper">

  <!-- Modal -->
  <div class="modal fade" id="myModal" role="dialog">
    <div class="modal-dialog">

      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 id="notificacaoMensagemTitle" class="modal-title"></h4>
        </div>
        <div class="modal-body">
          <p id="notificacaoMensagemBody"></p>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
          <button type="button" class="btn btn-primary" onclick="javascript:location.href = '/modelar_projeto'">Modelar Projeto</button>
        </div>
      </div>

    </div>
  </div>

  <!-- Content Header (Page header) -->
  <section class="content-header">
    <ol class="breadcrumb">
      <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
      <li class="active">Protótipos</li>
    </ol>
  </section>

            <!-- Main content -->
  <section class="content">
    <!-- Info boxes -->
    <div class="row">
      <div class="col-md-12">
        <div class="box">
          <div class="box-header with-border">
            <h3 class="box-title">Matheus o palestrante do dia</h3>

          </div><!-- /.box-header -->
          <div class="box-body">
            <div class="row">
             <!-- /.col -->
             <div class="col-md-12">
               <div class="box box-primary">
                 <div class="box-header">
                   <h3 class="box-title">Propostas recebida</h3>


                 </div>
                 <!-- /.box-header -->
                 <div class="box-body no-padding">
                   <table class="table">
                     <tbody><tr>
                       <th>Nome da Equipe</th>
                       <th>Apresentação</th>
                       <th>Dúvidas</th>
                       <th>Resposta</th>
                       <th>Aprovar/Recusar</th>
                       <!-- <th style="width: 40px">Função</th> -->
                     </tr>
                     <% for(var i=0; i < data.length; i++){ %>
                     <% var proposta = data[i].propostas[0] %>
                     <tr>
                       <td><%=proposta.nomeEquipe %></td>
                       <td><%=proposta.apresentacao %></td>
                       <td><%=proposta.duvidas %></td>
                       <td><span><%= proposta.feedback %></span></td>
                       <td>
                         <button type="button" class="btn btn-successo btn-sm"  value=">" onclick="" >Aprovar</button>
                         <button type="button" class="btn btn-danger btn-sm"  value=">" onclick="" >Recusar</button>

                       </td>
                       <!-- <td><span class="badge bg-red">Tutor</span></td> -->
                     </tr>
                     <% } %>
                   </tbody></table>
                 </div>
                 <!-- /.box-body -->
               </div>

             </div>
 <!-- /.col -->
</div>
          </div><!-- ./box-body -->

        </div><!-- /.box -->
      </div><!-- /.col -->
    </div><!-- /.row -->

  </section><!-- /.content -->
</div><!-- /.content-wrapper -->
<!-- <script src="plugins/jQuery/jQuery-2.1.3.min.js"></script> -->
