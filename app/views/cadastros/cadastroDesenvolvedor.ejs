<html lang="pt">
   <head>
      <meta charset="UTF-8">
      <title>SAGP | Cadastro Cliente</title>
      <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
      <!-- Bootstrap 3.3.2 -->
      <link href="bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
      <!-- Font Awesome Icons -->
      <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
      <!-- Theme style -->
      <link href="dist/css/AdminLTE.min.css" rel="stylesheet" type="text/css" />
      <!-- iCheck -->
      <link href="plugins/iCheck/square/blue.css" rel="stylesheet" type="text/css" />
      <link href="css/style_multiform.css" rel="stylesheet">
   </head>
   <body class="register-page">
      <div class="row">
         <form id="msform" method="post" action="/cadastrar_dev">
            <!-- progressbar -->
            <div class="col-md-offset-3 col-md-8 col-sm-offset-3">
              <ul id="progressbar" class="progressbarDesenvolvedor">
                 <li class="active">Informações Pessoais e de Contato</li>
                 <li>Dado Educacional</li>
                 <li>Conta</li>
              </ul>
            </div>
            <!-- fieldsets -->
            <div class="col-md-12">
            <fieldset>
               <!-- <div class="box box-success"> -->
                  <h2 class="fs-title">Informações Pessoais e de Contato</h2>
                  <!-- <h3 class="fs-subtitle">This is step 1</h3> -->

                  <div class="form-group col-md-3 ">
                     <input id="cpf" type="text" class="form-control validar cpf" name="cpf" placeholder="CPF *" />
                  </div>
                  <!-- col-md-offset-1 -->
                  <div class="form-group  col-md-6 ">
                     <input type="text" class="form-control validar" name="nome" placeholder="Digite seu nome completo *" />
                  </div>
                  <div class="form-group  col-md-3 ">
                     <input type="text" class="form-control validar" name="dataNascimento" placeholder="Data de Nascimento *" onfocus="tornarDate(this)" onblur="tornarText(this)" />
                  </div>
                  <div class="form-group col-md-6">
                    <input type="text" class="form-control" id="telefone" name="Telefone" placeholder="Telefone"/>
                  </div>

                  <div class="form-group col-md-6">
                    <input type="text" class="form-control" id="celular" name="celular" placeholder="Celular"/>
                  </div>
                  <div class="form-group col-md-8">
                    <input  id="email" type="email" class="form-control emailValidar"  maxlength="60"  data-placement="left" title="E-mail"  name="email" placeholder="E-mail *"/>
                  </div>
                  <!-- <div class="form-group col-md-4">
                    <input type="email" class="form-control emailValidar"  id="Hangouts" name="Hangouts" placeholder="Hangouts *" onblur="validarEmail(this, 'gmail.com', false)" />
                  </div>
                  <div class="form-group col-md-4">
                    <input type="text" class="form-control  emailValidar"   id="Skype" name="Skype" placeholder="Skype"  onblur="validarEmail(this, 'outlook.com', false)" />
                  </div> -->
                  <div class="form-group col-md-4">
                    <input type="text" class="form-control"   id="Github" name="GitHub" placeholder="GitHub"/>
                  </div>
                  <input type="button" name="proximo" class=" next action-button btn  btn-success" value="Próximo" align="center"/>
               <!-- </div> -->
            </fieldset>
            <fieldset>
              <h2 class="fs-title">Dados Educacionais</h2>

              <div class="form-group has-feedback col-md-9">
                <input type="text" class="form-control validar" id="instituicaoEnsino" name="instituicaoEnsino" placeholder="Instituição de Ensino *" />
              </div>
              <div class="form-group has-feedback col-md-3">
                <input type="text" class="form-control validar" id="matricula" name="matricula" placeholder="Matrícula *"/>
              </div>
              <div class="form-group has-feedback col-md-6">
                <input type="text" class="form-control validar" id="curso" name="curso" placeholder="Curso *"/>
              </div>
              <div class="form-group has-feedback col-md-3">
                <select class="form-control validarSelect validar" id="nivel" name="nivel" placeholder="Turno *">
               	 <option value="">Turno</option>
                  <option value="Manha">Manhã</option>
                  <option value="Tarde">Tarde</option>
                  <option value="Noite">Noite</option>
                  <option value="Integral">Integral</option>
                </select>
              </div>
              <div class="form-group has-feedback col-md-3">
                <select class="form-control validarSelect validar" id="periodo" name="nivperiodo" placeholder="Período *">
               	 <option value="">Período</option>
                  <option value="1">1°</option>
                  <option value="2">2°</option>
                  <option value="3">3°</option>
                  <option value="4">4°</option>
                  <option value="5">5°</option>
                  <option value="6">6°</option>
                  <option value="7">7°</option>
                  <option value="8">8°</option>
                  <option value="9">9°</option>
                  <option value="10">10°</option>
                  <option value="11">11°</option>
                  <option value="12">12°</option>
                </select>
              </div>
              <div class="col-md-12">
                <div class="custom-file">
                  <span class="btn btn-default btn-bloc">Declaração de Matrícula (Em PDF ou JPG) *</span>
                  <input  type="file" class="custom-file-input btn btn-danger validar" id="customFile" name="DocDeclaracaoMatricula" placeholder="Declaração de Mátricula (Em PDF ou JPG)">
                </div>
            </div>


               <input type="button" name="Anterior" class="previous action-button  btn  btn-success" value="Anterior" />
               <input type="button" name="proximo" class=" next action-button btn  btn-success" value="Próximo"/>
            </fieldset>

          <!-- ------------------------------   obs  ------------------------------
          ADD UMA NOVA ETAPA COM AS EXPERIENCIAS DOS ALUNOS EX. SISTEMAS DESENVOLVIDOS, TECNOLOGIA QUE JÁ ESTODOU -->
            <fieldset>
               <h2 class="fs-title">Conta</h2>
               <div class="form-group col-md-12 ">
                  <input id="emailLabel" type="text" class="form-control" name="email" placeholder="Email"  readonly="true"  />
               </div>
               <!-- <div class="col-md-12 "> -->
                  <div class="form-group col-md-6">
                     <input  id="senha" type="password" class="form-control validar validarSenha" name="senha" placeholder="Senha" />
                  </div>
                  <div class="form-group col-md-6">
                     <input id="csenha" type="password" class="form-control validar validarSenha" name="csenha" placeholder="Confirme a Senha" />
                  </div>
               <!-- </div> -->
               <input type="button" name="Anterior" class="previous action-button   btn  btn-success" value="Anterior" />
               <!-- <input type="submit" name="Pronto" class="submit action-button" value="Cadastrar" /> -->
               <input type="submit" name="Pronto" class="submit action-button btn  btn-success" value="Cadastrar" />
            </fieldset>
            </div>
         </form>
         <div id="msg"></div>
      </div>
      <!-- jQuery -->
      <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js" type="text/javascript"></script>
      <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
      <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
      <!-- jQuery easing plugin -->
      <script src="http://thecodeplayer.com/uploads/js/jquery.easing.min.js" type="text/javascript"></script>
      <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.0/jquery.mask.js"></script>
      <script src="js/script_multiform_dev.js" type="text/javascript"></script>
      <script src="js/valida_cpf_cnpj.js" type="text/javascript"></script>
      <script>
      //Máscaras dos campos
      $(document).ready(function () {
        $('[data-toggle="tooltip"]').tooltip();
        var $cpf = $("#cpf");
        $cpf.mask('000.000.000-00', {reverse: false});

        var $telefone = $("#telefone");
        $telefone.mask('(00)0000-0000', {reverse: false});

        var $celular = $("#celular");
        $celular.mask('(00)00000-0000', {reverse: false});
      });

    function tornarDate(dataNascimento){
        dataNascimento.type = 'date';
     }
     function tornarText(dataNascimento){
       if(!dataNascimento.value != ''){
         dataNascimento.type = 'text';
       }
     }

    </script>
   </body>
</html>
