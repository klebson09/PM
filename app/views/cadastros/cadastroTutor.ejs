<html lang="pt">
   <head>
      <meta charset="UTF-8">
      <title>SAGP | Cadastro Cliente</title>
      <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
      <!-- Bootstrap 3.3.2 -->
      <link href="bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
      <!-- Font Awesome Icons -->
      <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
      <!-- Theme style -->
      <link href="dist/css/AdminLTE.min.css" rel="stylesheet" type="text/css" />
      <!-- iCheck -->
      <link href="plugins/iCheck/square/blue.css" rel="stylesheet" type="text/css" />
      <link href="css/style_multiform.css" rel="stylesheet">
   </head>
   <body class="register-page">
      <div class="row">
         <form id="msform" action="/cadastrar_tutor" method="post">
            <!-- progressbar -->
            <div class="col-md-offset-3 col-md-8">
            <ul id="progressbar">
               <li class="active">Informações Pessoais e de Contato</li>
               <li>Dado Educacional</li>
               <li>Conta</li>
            </ul>
            </div>
            <!-- fieldsets -->
            <div class="col-md-12">
            <fieldset>
               <!-- <div class="box box-success"> -->
                  <h2 class="fs-title">Informações Pessoais e de Contato</h2>
                  <div class="form-group col-md-3 ">
                     <input type="text" class="form-control" name="cpf" placeholder="CPF" />
                  </div>
                  <!-- col-md-offset-1 -->
                  <div class="form-group  col-md-6 ">
                     <input type="text" class="form-control" name="nome" placeholder="Digite seu nome completo" />
                  </div>
                  <div class="form-group  col-md-3 ">
                     <input type="date" class="form-control" name="dataNascimento" placeholder="Data de Nascimento" />
                  </div>
                  <div class="form-group col-md-4">
                    <input type="text" class="form-control" id="Telefone" name="Telefone" placeholder="Telefone"/>
                  </div>
                  <div class="form-group col-md-4">
                    <input type="email" class="form-control"  id="Hangouts" name="Hangouts" placeholder="Hangouts"/>
                  </div>
                  <div class="form-group col-md-4">
                    <input type="text" class="form-control"   id="Skype" name="Skype" placeholder="Skype"/>
                  </div>
                  <input type="button" name="proximo" class=" next action-button btn  btn-success" value="Próximo" align="center"/>
               <!-- </div> -->
            </fieldset>
            <fieldset>
              <h2 class="fs-title">Dados Educacional</h2>

              <div class="form-group has-feedback col-md-9">
                <input type="text" class="form-control" id="instituicaoEnsino" name="instituicaoEnsino" placeholder="Instituição de Ensino" />
              </div>
              <div class="form-group has-feedback col-md-3">
                <input type="text" class="form-control" id="siape" name="siape" placeholder="SIAPE"/>
              </div>
              <div class="form-group has-feedback col-md-9">
                <input type="text" class="form-control" id="formacaoAcademica" name="formacaoAcademica" placeholder="Formação Acadêmica"/>
              </div>
              <div class="form-group has-feedback col-md-3">
                <select class="form-control" id="nivel" name="cargo">
               	 <option value="">Cargo</option>
                  <option value="Professor">Professor</option>
                  <option value="Gestor">Gestor</option>
                  <option value="AlunoVeterano">Aluno Veterano</option>
                  <option value="Outros">Outros</option>
                </select>
              </div>

              <!-- <div class="col-md-12">
                <div class="custom-file">
                  <span class="btn btn-default btn-bloc">Declaração de Matrícula (Em PDF ou JPG)</span>
                  <input  type="file" class="custom-file-input btn btn-danger" id="customFile">
                </div>
              </div> -->


               <input type="button" name="Anterior" class="previous action-button  btn  btn-success" value="Anterior" />
               <input type="button" name="proximo" class=" next action-button btn  btn-success" value="Próximo"/>
            </fieldset>

          <!-- ------------------------------   obs  ------------------------------
          ADD UMA NOVA ETAPA COM AS EXPERIENCIAS DOS ALUNOS EX. SISTEMAS DESENVOLVIDOS, TECNOLOGIA QUE JÁ ESTODOU -->
            <fieldset>
               <h2 class="fs-title">Conta</h2>
               <div class="form-group col-md-12 ">
                  <input type="text" class="form-control" name="email" placeholder="Email" />
               </div>
               <!-- <div class="col-md-12 "> -->
                  <div class="form-group col-md-6">
                     <input type="password" class="form-control" name="senha" placeholder="Senha" />
                  </div>
                  <div class="form-group col-md-6">
                     <input type="password" class="form-control" name="csenha" placeholder="Confirme a Senha" />
                  </div>
               <!-- </div> -->
               <input type="button" name="Anterior" class="previous action-button   btn  btn-success" value="Anterior" />
               <!-- <input type="submit" name="Pronto" class="submit action-button" value="Cadastrar" /> -->
               <input type="submit" name="Pronto" class="submit action-button btn  btn-success" value="Cadastrar" />
            </fieldset>
            </div>
         </form>
      </div>
      <!-- jQuery -->
      <script src="http://thecodeplayer.com/uploads/js/jquery-1.9.1.min.js" type="text/javascript"></script>
      <!-- jQuery easing plugin -->
      <script src="http://thecodeplayer.com/uploads/js/jquery.easing.min.js" type="text/javascript"></script>
      <script src="js/script_multiform.js" type="text/javascript"></script>

   </body>
</html>
